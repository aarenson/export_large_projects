{
    "name": "Export large projects",
    "namespace": "ExportLargeProjects\\ExternalModule",
    "description": "This module enables REDCap to export large projects without getting a timeout. This module is developed to bring Stanford University's Large Project Export plugin to REDCap's external modules. See the <b><a href='https://github.com/ctsit/export_large_projects' target='_blank'>documentation</a></b> at <a href='https://github.com/ctsit/export_large_projects' target='_blank'>https://github.com/ctsitexport_large_projects</a>",
    "permissions": [
        "hook_every_page_top"
    ],
    "authors": [
        {
            "name": "Surya Prasanna",
            "email": "suryayalla@ufl.edu",
            "institution": "CTS-IT - University of Florida"
        },
        {
            "name": "Philip Chase",
            "email": "pbc@ufl.edu",
            "institution": "CTS-IT - University of Florida"
        },
        {
            "name": "Taryn Stoffs",
            "email": "tls@ufl.edu",
            "institution": "CTS-IT - University of Florida"
        }
    ],
    "project-settings": [
        {
            "key": "fields-per-batch",
            "name": "No. of fields per batch",
            "type": "text",
            "default": "8000"
        },
        {
            "key": "max-execution-time",
            "name": "Max execution time",
            "type": "text",
            "default": "1200"
        }
    ],
    "compatibility": {
        "redcap-version-min": "8.0.3",
        "redcap-version-max": ""
   }
}
